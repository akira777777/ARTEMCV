}
.loading-dots div:nth-child(1) {
  left: 8px;
  animation: loading1 0.6s infinite;
}
.loading-dots div:nth-child(2) {
  left: 8px;
  animation: loading2 0.6s infinite;
}
.loading-dots div:nth-child(3) {
  left: 32px;
  animation: loading2 0.6s infinite;
}
.loading-dots div:nth-child(4) {
  left: 56px;
  animation: loading3 0.6s infinite;
}
@keyframes loading1 {
  0% { transform: scale(0); }
  100% { transform: scale(1); }
}
@keyframes loading2 {
  0% { transform: translate(0, 0); }
  100% { transform: translate(24px, 0); }
}
@keyframes loading3 {
  0% { transform: scale(1); }
  100% { transform: scale(0); }
}
/* Smooth State Transitions */
.smooth-transition {
  transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
}
.smooth-transition-fast {
  transition: all 0.15s cubic-bezier(0.4, 0, 0.2, 1);
}
.smooth-transition-slow {
  transition: all 0.6s cubic-bezier(0.4, 0, 0.2, 1);
}
/* Enhanced smooth transitions with different easing functions */
.ease-smooth {
  transition: all 0.4s cubic-bezier(0.25, 0.46, 0.45, 0.94);
}
.ease-bounce {
  transition: all 0.5s cubic-bezier(0.68, -0.55, 0.265, 1.55);
}
.ease-elastic {
  transition: all 0.6s cubic-bezier(0.68, -0.55, 0.27, 1.55);
}
.ease-spring {
  transition: all 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);
}
.ease-anticipation {
  transition: all 0.4s cubic-bezier(0.38, 0.1, 0.36, 0.9);
}
.ease-cosmic {
  transition: all 0.6s cubic-bezier(0.33, 1, 0.68, 1);
}
/* Hover and interaction states with enhanced timing */
.interactive-element {
  transition: all 0.35s cubic-bezier(0.34, 1.56, 0.64, 1);
}
.interactive-element:hover {
  transform: translateY(-2px);
  box-shadow: 0 10px 25px rgba(0, 0, 0, 0.2);
}
/* Delayed transitions for staggered animations */
.delay-100 { transition-delay: 0.1s; }
.delay-200 { transition-delay: 0.2s; }
.delay-300 { transition-delay: 0.3s; }
.delay-400 { transition-delay: 0.4s; }
.delay-500 { transition-delay: 0.5s; }
.delay-600 { transition-delay: 0.6s; }
.delay-700 { transition-delay: 0.7s; }
.delay-800 { transition-delay: 0.8s; }
.delay-900 { transition-delay: 0.9s; }
.delay-1000 { transition-delay: 1.0s; }
/* Interactive Feedback States */
.feedback-success {
  animation: feedbackSuccess 0.6s cubic-bezier(0.4, 0, 0.2, 1);
}
.feedback-error {
  animation: feedbackError 0.6s cubic-bezier(0.4, 0, 0.2, 1);
}
@keyframes feedbackSuccess {
  0% { transform: scale(1); }
  50% { transform: scale(1.05); }
  100% { transform: scale(1); }
}
@keyframes feedbackError {
  0%, 100% { transform: translateX(0); }
  25% { transform: translateX(-5px); }
  75% { transform: translateX(5px); }
}
/* Cursor Interaction Effects */
.cursor-pointer {
  cursor: pointer;
}
cursor-pointer:hover {
  filter: brightness(1.1);
}
cursor-pointer:active {
  transform: scale(0.95);
}
/* Scroll-triggered Animations */
.scroll-fade-in {
  opacity: 0;
  transform: translateY(30px);
  transition: 
    opacity 0.8s cubic-bezier(0.4, 0, 0.2, 1),
    transform 0.8s cubic-bezier(0.4, 0, 0.2, 1);
}
.scroll-fade-in.visible {
  opacity: 1;
  transform: translateY(0);
}
/* Staggered Animations */
.stagger-delay-1 { animation-delay: 0.1s; }
.stagger-delay-2 { animation-delay: 0.2s; }
.stagger-delay-3 { animation-delay: 0.3s; }
.stagger-delay-4 { animation-delay: 0.4s; }
.stagger-delay-5 { animation-delay: 0.5s; }
/* Touch Device Optimizations */
@media (hover: none) and (pointer: coarse) {
  .interactive-button:hover,
  .interactive-link:hover,
  .interactive-card:hover {
    transform: none;
  }
  .interactive-button:active,
  .interactive-link:active,
  .interactive-card:active {
    transform: scale(0.98);
  }
}
/* Focus Visible Enhancements */
.focus-visible-ring {
  outline: 2px solid transparent;
  outline-offset: 2px;
}
.focus-visible-ring:focus-visible {
  outline: 2px solid hsl(var(--color-primary));
  outline-offset: 2px;
  box-shadow: 0 0 0 4px hsl(var(--color-primary) / 0.3);
}
