    to right, 
    var(--primary) 0%, 
    var(--secondary) 35%, 
    var(--accent) 65%, 
    var(--supporting) 100%
  );
  z-index: 10000;
  transition: width 0.1s ease;
  box-shadow: 0 0 8px rgba(14, 165, 233, 0.6);
  border-radius: 0 2px 2px 0;
}
/* Decorative Contact Form Borders */
.decorative-border-left,
.decorative-border-right {
  position: absolute;
  top: 0;
  bottom: 0;
  width: 3px;
  pointer-events: none;
  z-index: 20;
}
.decorative-border-left {
  left: 0;
  background: linear-gradient(
    to bottom,
    var(--primary),
    var(--secondary),
    var(--accent),
    var(--supporting)
  );
  box-shadow: 
    0 0 20px rgba(14, 165, 233, 0.6),
    inset 0 0 10px rgba(255, 255, 255, 0.1);
}
.decorative-border-right {
  right: 0;
  background: linear-gradient(
    to bottom,
    var(--supporting),
    var(--accent),
    var(--secondary),
    var(--primary)
  );
  box-shadow: 
    0 0 20px rgba(139, 92, 246, 0.6),
    inset 0 0 10px rgba(255, 255, 255, 0.1);
}
/* Contact form inner border effect */
.form-inner-border {
  position: absolute;
  inset: 0;
  border: 1px solid var(--primary);
  border-radius: inherit;
  opacity: 0.3;
  box-shadow: 
    inset 0 0 30px rgba(16, 185, 129, 0.1),
    inset 0 0 60px rgba(6, 182, 212, 0.05);
  pointer-events: none;
}
/* Enhanced form inputs */
.contact-input {
  background: rgba(17, 24, 39, 0.3);
  border: 1px solid rgba(14, 165, 233, 0.3);
  transition: all 0.3s cubic-bezier(0.23, 1, 0.32, 1);
  border-radius: 12px;
  padding: 12px 16px;
  color: var(--text-primary);
}
/* Enhanced visual depth and layering */
.visual-depth-1 {
  transform: translateZ(0px);
  filter: blur(0px);
  opacity: 1;
}
.visual-depth-2 {
  transform: translateZ(10px);
  filter: blur(0.5px);
  opacity: 0.95;
}
.visual-depth-3 {
  transform: translateZ(20px);
  filter: blur(1px);
  opacity: 0.9;
}
.visual-depth-4 {
  transform: translateZ(30px);
  filter: blur(1.5px);
  opacity: 0.85;
}
.visual-depth-5 {
  transform: translateZ(40px);
  filter: blur(2px);
  opacity: 0.8;
}
/* Smooth parallax effect */
.parallax-smooth {
  transform: translateZ(0) scale(1);
  transition: transform 0.2s ease-out;
}
.parallax-smooth:hover {
  transform: translateZ(-20px) scale(1.02);
}
/* Enhanced glow effects */
.glow-soft {
  box-shadow: 0 0 15px rgba(14, 165, 233, 0.2);
}
.glow-medium {
  box-shadow: 0 0 25px rgba(14, 165, 233, 0.3);
}
.glow-strong {
  box-shadow: 0 0 40px rgba(14, 165, 233, 0.4);
}
.glow-pulse {
  animation: glowPulse 2s ease-in-out infinite alternate;
}
@keyframes glowPulse {
  from { box-shadow: 0 0 15px rgba(14, 165, 233, 0.2); }
  to { box-shadow: 0 0 30px rgba(14, 165, 233, 0.4); }
}
.contact-input:focus {
  border-color: rgba(14, 165, 233, 0.6);
  outline: none;
  background: rgba(17, 24, 39, 0.5);
  box-shadow: 
    0 0 0 3px rgba(14, 165, 233, 0.2),
    0 0 20px rgba(14, 165, 233, 0.3),
    inset 0 0 0 1px rgba(14, 165, 233, 0.1);
  transform: translateY(-1px);
}
.contact-input::placeholder {
  color: var(--text-tertiary);
  font-weight: 400;
}
/* Contact section labels */
.contact-label {
  color: var(--primary);
  font-weight: 600;
  font-size: 0.875rem;
  letter-spacing: 0.02em;
  text-transform: uppercase;
}
/* Contact social links */
.contact-social-link {
  border: 1px solid rgba(14, 165, 233, 0.3);
  color: var(--primary);
  transition: all 0.3s cubic-bezier(0.23, 1, 0.32, 1);
  border-radius: 12px;
  padding: 12px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.contact-social-link:hover {
  background: linear-gradient(45deg, rgba(14, 165, 233, 0.15), rgba(16, 185, 129, 0.15));
  border-color: rgba(14, 165, 233, 0.6);
  color: var(--primary-light);
  transform: translateY(-2px);
  box-shadow: 0 8px 20px rgba(14, 165, 233, 0.2);
}
/* Improved responsive adjustments for better scaling */
@media (max-width: 768px) {
  .decorative-border-left,
  .decorative-border-right {
    width: 2px;
  }
  .decorative-border-left {
    box-shadow: 0 0 10px rgba(16, 185, 129, 0.3);
  }
  .decorative-border-right {
    box-shadow: 0 0 10px rgba(245, 158, 11, 0.3);
  }
  /* Additional mobile optimizations */
  .text-4xl { font-size: clamp(1.5rem, 5vw, 2.5rem); }
  .text-5xl { font-size: clamp(1.8rem, 6vw, 3rem); }
  .text-6xl { font-size: clamp(2rem, 7vw, 3.5rem); }
  /* Scale down large elements */
