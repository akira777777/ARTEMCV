/* ==================== NEW ANIMATIONS ==================== */
/* Parallax floating elements */
.parallax-layer {
  position: absolute;
  will-change: transform;
  pointer-events: none;
}
.parallax-bg {
  transform: translateZ(-2px) scale(1.2);
}
.parallax-mid {
  transform: translateZ(-1px) scale(1.1);
}
.parallax-fg {
  transform: translateZ(0);
}
/* 3D Card tilt effect */
.card-3d {
  perspective: 1000px;
  transform-style: preserve-3d;
}
.card-3d-inner {
  transition: transform 0.4s ease-out;
  transform-style: preserve-3d;
  will-change: transform;
}
.card-3d:hover .card-3d-inner {
  transform: rotateX(5deg) rotateY(5deg) translateZ(20px);
}
/* Gradient text animation */
.gradient-text {
  background: linear-gradient(
    90deg,
    var(--primary) 0%,
    var(--secondary) 25%,
    var(--accent) 50%,
    var(--supporting) 75%,
    var(--primary) 100%
  );
  -webkit-background-clip: text;
  background-clip: text;
  color: transparent;
  background-size: 400% 100%;
  animation: gradientShift 6s cubic-bezier(0.4, 0, 0.2, 1) infinite;
  text-shadow: 0 2px 10px rgba(0, 0, 0, 0.2);
}
.gradient-text-subtle {
  background: linear-gradient(
    90deg,
    var(--primary-light) 0%,
    var(--secondary-light) 50%,
    var(--primary-light) 100%
  );
  -webkit-background-clip: text;
  background-clip: text;
  color: transparent;
  background-size: 300% 100%;
  animation: gradientShift 8s cubic-bezier(0.4, 0, 0.2, 1) infinite;
  text-shadow: 0 1px 5px rgba(0, 0, 0, 0.1);
}
@keyframes gradientShift {
  0% { background-position: 0% 50%; }
  50% { background-position: 100% 50%; }
  100% { background-position: 0% 50%; }
}
@keyframes gradientShiftReverse {
  0% { background-position: 100% 50%; }
  50% { background-position: 0% 50%; }
  100% { background-position: 100% 50%; }
}
@keyframes gradientShiftReverse {
  0% { background-position: 100% 50%; }
  50% { background-position: 0% 50%; }
  100% { background-position: 100% 50%; }
}
/* Interactive blob cursor effect */
.interactive-blob {
  position: fixed;
  width: 20px;
  height: 20px;
  background: radial-gradient(circle, var(--primary), transparent 70%);
  border-radius: 50%;
  pointer-events: none;
  z-index: 9999;
  mix-blend-mode: difference;
  transform: translate(-50%, -50%);
  transition: width 0.3s, height 0.3s, background 0.3s;
}
.interactive-blob.hover {
  width: 40px;
  height: 40px;
  background: radial-gradient(circle, var(--accent), transparent 70%);
}
/* Scroll-triggered animations */
.scroll-fade-in {
  opacity: 0;
  transform: translateY(30px);
  transition: opacity 0.8s ease, transform 0.8s ease;
}
.scroll-fade-in.visible {
  opacity: 1;
  transform: translateY(0);
}
/* Enhanced floating particles with depth */
.particle {
  position: absolute;
  border-radius: 50%;
  background: radial-gradient(
    circle at 30% 30%,
    var(--primary-light),
    var(--primary)
  );
  opacity: 0.7;
  animation: float 20s infinite cubic-bezier(0.34, 1.56, 0.64, 1);
  pointer-events: none;
  box-shadow: 
    0 0 20px var(--primary),
    inset 0 0 10px rgba(255, 255, 255, 0.3);
  filter: blur(0.5px);
}
@keyframes float {
  0% { 
    transform: translateY(0) translateX(0) scale(0.8); 
    opacity: 0; 
  }
  15% { 
    opacity: 0.7; 
    transform: scale(1); 
  }
  85% { 
    opacity: 0.7; 
    transform: scale(1.1); 
  }
  100% { 
    transform: translateY(-100vh) translateX(30px) scale(0.7); 
    opacity: 0; 
  }
}
/* Smoother float animation with bouncier easing */
@keyframes float-smooth {
  0% { 
    transform: translateY(0) translateX(0) scale(0.8); 
    opacity: 0; 
  }
  10% { 
    opacity: 0.7; 
